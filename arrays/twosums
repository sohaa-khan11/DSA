''' 
1. Two Sum
link : https://leetcode.com/problems/two-sum/?envType=problem-list-v2&envId=array

answer:
'''
class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        seen={}
        for i, n in enumerate(nums):
            diff=target-n
            if diff in seen:
                return [seen[diff], i]
            seen[n]=i